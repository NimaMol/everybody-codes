@page "/Map"
@using Microsoft.JSInterop
@using Radzen;
@using Radzen.Blazor
@inject IJSRuntime JSRuntime

<div class="container-fluid">
    <div class="row p-3">
        <div class="col">
                <RadzenGoogleMap  style="height: 400px" Options=@(new Dictionary<string, object> { { "disableDoubleClickZoom", true } })
