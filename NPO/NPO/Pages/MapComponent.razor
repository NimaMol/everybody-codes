                    {
                        <RadzenGoogleMapMarker Title="CameraName" Label="London" Position=@(new GoogleMapPosition() { Lat = Convert.ToDouble(cam.lat), Lng = Convert.ToDouble(cam.lon) }) />
                    }*@
              </Markers>
            </RadzenGoogleMap>
        </div>
    </div>
</div>



